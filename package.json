{
  "name": "parse-structured-data-action",
  "description": "Helper action to parse structured data in comment strings from issue comments",
  "version": "0.1.0",
  "author": "Sean Dunn <sean@attamusc.com>",
  "type": "module",
  "private": true,
  "homepage": "https://github.com/attamusc/parse-structured-data-action",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/attamusc/parse-structured-data-action.git"
  },
  "bugs": {
    "url": "https://github.com/attamusc/parse-structured-data-action/issues"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "bundle": "npm run format:write && npm run package",
    "ci-test": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 npx jest",
    "format:write": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "lint": "npx eslint .",
    "package": "npx rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 npx jest",
    "all": "npm run format:write && npm run lint && npm run test && npm run package"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@octokit/graphql": "^8.1.1",
    "@octokit/rest": "^21.0.0",
    "mdast-comment-marker": "^3.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.5",
    "unist": "^0.0.1",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@octokit/graphql-schema": "^15.25.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.14.7",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vercel/ncc": "^0.38.2",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-github": "^5.0.2",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "rollup": "^4.22.5",
    "ts-jest": "^29.2.5",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
